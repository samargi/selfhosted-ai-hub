# === Domain for Caddy ==============================================
DOMAIN=example.domain.com

# === Core ==========================================================
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_secure_password_123
POSTGRES_DB=your_database_name
MINIO_ROOT_USER=your_minio_user
MINIO_ROOT_PASSWORD=your_secure_password_123

# === LangChain-API / Vector / Cache / Storage ======================
LANGCHAIN_API_KEY=your_langchain_api_key_here
LITELLM_BASE_URL=http://litellm:4000/v1
QDRANT_URL=http://qdrant:6333
REDIS_URL=redis://redis:6379/0
MINIO_ENDPOINT=http://minio:9000
LITELLM_MODEL=gpt-4o

# Use this to auth against LiteLLM from services that expect OPENAI_API_KEY
LITELLM_UPSTREAM_API_KEY=sk-your_litellm_api_key_here

# === Flowise =======================================================
FLOWISE_USERNAME=your_flowise_username
FLOWISE_PASSWORD=your_flowise_password
OPENAI_BASE_PATH=http://litellm:4000/v1

# === n8n (namespaced) ==============================================
N8N_DB_TYPE=postgresdb
N8N_DB_POSTGRESDB_HOST=postgres
N8N_DB_POSTGRESDB_PORT=5432
N8N_DB_POSTGRESDB_DATABASE=n8n
N8N_DB_POSTGRESDB_USER=your_db_user
N8N_DB_POSTGRESDB_PASSWORD=your_secure_password_123
N8N_ENCRYPTION_KEY=your_n8n_encryption_key
N8N_HOST=n8n.example.domain.com
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_EDITOR_BASE_URL=https://n8n.example.domain.com/
WEBHOOK_URL=https://n8n.example.domain.com/
N8N_SECURE_COOKIE=true
GENERIC_TIMEZONE=Europe/Helsinki
N8N_PROXY_HOPS=1
N8N_TRUSTED_PROXIES=0.0.0.0/0,::/0
N8N_RUNNERS_ENABLED=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_USER_MANAGEMENT_DISABLED=
N8N_USER_MANAGEMENT_EMAIL=
N8N_USER_MANAGEMENT_PASSWORD=

# === LiteLLM =======================================================
UI_USERNAME=admin
UI_PASSWORD=your_ui_admin_password
PROXY_ADMIN_ID=admin
LITELLM_LOCAL_MODEL_COST_MAP=true

# === Open WebUI ====================================================
OPENWEBUI_OPENAI_API_KEY=sk-proj-your_openai_api_key_here
WEBUI_AUTH=True
DATA_DIR=/app/backend/data
FRONTEND_BUILD_DIR=/app/build
STATIC_DIR=/app/backend/static
ENABLE_WEB_SEARCH=True
WEB_SEARCH_ENGINE=perplexity
PERPLEXITY_API_KEY=pplx-your_perplexity_api_key_here
OPENWEBUI_DEFAULT_MODELS_VISIBILITY=public
OPENAI_API_BASE_URLS=http://litellm:4000,http://pipelines:9099
OPENAI_API_KEYS=sk-or-v1-your_api_key_1,your_api_key_2
ENABLE_FOLLOW_UP_GENERATION=False
DEFAULT_PROMPT_SUGGESTIONS=[]
ENABLE_PERSISTENT_CONFIG=True
WEBUI_SECRET_KEY=your_webui_secret_key
WEBUI_NAME=YourAppName
ENABLE_CHANNELS=True
ENABLE_COMMUNITY_SHARING=False
ENABLE_FORWARD_USER_INFO_HEADERS=True
VECTOR_DB=qdrant
QDRANT_URI=http://qdrant:6333
QDRANT_API_KEY=
PIPELINES_URLS="http://pipelines:9099"
ENABLE_OPENAI_API="true"

# === Pipelines =====================================================
MIN_UPDATES=3
MAX_UPDATES=6
MAX_THINK_TOKENS=512
MAX_ANSWER_TOKENS=1024
UPSTREAM_BASE_URL=http://litellm:4000
UPSTREAM_API_KEY=sk-your_upstream_api_key_here
THINK_MODEL=o4-mini
RESP_MODEL=None
PIPELINES_DIR=/app/pipelines
PIPELINES_API_KEY=